{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "no-console": ["warn", { "allow": ["error"] }], 
        "quotes": ["error", "single"], 
        "indent": ["warn", 4], 
        "max-len": ["warn", {"code": 140}], 
        "comma-dangle": ["error", "never"], 
        "semi": ["warn", "always"]
    }
}